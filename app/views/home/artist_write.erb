<div class="container" style="min-height:1100px; margin-top:70px;">
   <div class="main_title normal-font" >
        <h2 style="text-align:left;">아티스트 추가</h2>
   </div>
     <form action= "/home/artist_upload" method= "POST"  enctype="multipart/form-data" accept-charset="UTF-8">
         <div class="mir_input_div">
             <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
             <div class="mir_input1">
                 <input type="text" placeholder="이름을 입력해주세요" style="padding-left:10px;" class="inquire_name" name="name">
             </div>
             <div class="mir_input1">
                 <input type="text" placeholder="중문 이름을 입력해주세요" style="padding-left:10px;" class="inquire_name" name="name_c">
             </div>
             <div class="mir_input_file">
                <input type="file" placeholder="사진을 업로드 해주세요"  style="padding-left:10px;" class="inquire_file" name="pic">
             </div>
             <div class="mir_input2">
                <textarea rows="10" placeholder="내용을 입력해주세요" style="padding-left:10px;" class="inquire_content" name="bio"></textarea>
             </div>
             <div class="mir_input2">
                <textarea rows="10" placeholder="중문 내용을 입력해주세요" style="padding-left:10px;" class="inquire_content" name="bio_c"></textarea>
             </div>
              <button type="submit" class="btn btn-success" style="margin:10px auto"> 등록하기</button>
         </div>
     </form>
</div>